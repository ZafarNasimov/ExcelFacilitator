@using ExcelFacilitator.Models.ViewModels
@model List<AnalyticsResult>

<table class="table table-sm table-bordered mt-3">
    <thead class="table-light">
        <tr>
            <th>#</th>
            <th>Key</th>
            <th>Value</th>
            <th>Percentage (%)</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i].Key</td>
                <td>@Model[i].Value</td>
                <td>@Model[i].Percentage</td>
            </tr>
        }
    </tbody>
</table>
